@{
    ViewData["Title"] = "Home Page";
}

<div class="container-fluid">
    <div id="carouselExampleIndicators" class="carousel slide" data-ride="carousel">
        <div class="row w-100 mx-auto" role="listbox">
            <div class="carousel-inner">

            </div>
        </div>
        <a class="carousel-control-prev" onclick="next('right')">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="sr-only">Previous</span>
        </a>
        <a class="carousel-control-next" href="#carouselExampleIndicators" role="button" onclick="next('left')">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="sr-only">Next</span>
        </a>
    </div>
</div>

<script src="~/js/populateCarousel.js" asp-append-version="true"></script>
